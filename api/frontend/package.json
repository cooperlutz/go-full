{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "pre-commit": "pnpm deps && pnpm type-check && pnpm lint && pnpm format",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "deps": "pnpm i && pnpm audit && pnpm declutter",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly",
    "declutter": "knip --no-exit-code",
    "test": "vitest --dom",
    "test:once": "vitest --run --coverage --dom",
    "coverage": "vitest --coverage --dom",
    "test-ui": "vitest --ui",
    "openapi": "openapi-generator-cli --openapitools .openapitools.json generate"
  },
  "dependencies": {
    "@lucide/lab": "^0.1.2",
    "@tailwindcss/vite": "^4.1.14",
    "@vueuse/core": "^13.9.0",
    "chart.js": "^4.5.0",
    "daisyui": "^5.1.27",
    "lucide-vue-next": "^0.545.0",
    "marked": "^16.3.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "sweetalert2": "^11.23.0",
    "tailwindcss": "^4.1.14",
    "uuid": "^13.0.0",
    "vue": "^3.5.21",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.24.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.7.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-standard-with-typescript": "^9.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.2",
    "knip": "^5.64.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  }
}
