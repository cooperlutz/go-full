{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "pre-commit": "pnpm deps && pnpm type-check && pnpm lint && pnpm format",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "deps": "pnpm i && pnpm audit && pnpm declutter",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --declaration --emitDeclarationOnly",
    "declutter": "knip --no-exit-code",
    "test": "vitest --dom --reporter verbose",
    "test:once": "vitest --run --coverage --dom",
    "coverage": "vitest --coverage --dom",
    "test-ui": "vitest --ui",
    "openapi": "openapi-generator-cli --openapitools .openapitools.json generate"
  },
  "dependencies": {
    "@lucide/lab": "^0.1.2",
    "@tailwindcss/vite": "^4.1.18",
    "@vueuse/core": "^14.1.0",
    "chart.js": "^4.5.1",
    "daisyui": "^5.5.8",
    "lucide-vue-next": "^0.562.0",
    "marked": "^17.0.0",
    "sweetalert2": "^11.26.17",
    "tailwindcss": "^4.1.18",
    "uuid": "^13.0.0",
    "vue": "^3.5.25",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.25.2",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vitest/coverage-v8": "4.0.10",
    "@vitest/eslint-plugin": "^1.5.2",
    "@vitest/ui": "^4.0.10",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-standard-with-typescript": "^9.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^16.5.0",
    "happy-dom": "^20.3.0",
    "knip": "^5.80.2",
    "msw": "2.12.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.10",
    "vue-tsc": "^3.1.4"
  }
}
