{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.14.0",
    "generators": { 
      "pingpong": { 
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/src/pingpong/services",
        "inputSpec": "../rest/pingpong/v1/api.yaml",
        "additionalProperties": {
          "withInterfaces": true
        }
      }
    }
  }
}
