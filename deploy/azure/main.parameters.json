{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "name": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "dbPassword": {
        "value": "${DB_PASSWORD}"
      },
      "containerRegPat": {
        "value": "${CONTAINER_REG_PAT}"
      },
      "containerRegUserName": {
        "value": "${CONTAINER_REG_USERNAME}"
      },
      "applicationName": {
        "value": "${APP_NAME}"
      },
      "allowedInboundIP" : {
        "value": "${INBOUND_IP_ALLOW}"
      },
      "databaseAdminUser": {
        "value": "${DB_USER}"
      },
      "databaseType": {
        "value": "${DB_TYPE}"
      },
      "databaseName": {
        "value": "${DB_NAME}"
      },
      "databaseSSLMode": {
        "value": "${DB_SSLMODE}"
      },
      "httpPort": {
        "value": "${HTTP_PORT}"
      },
      "imageTag": {
        "value": "${IMAGE_TAG}"
      }
    }
  }